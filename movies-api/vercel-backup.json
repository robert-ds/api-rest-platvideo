{
  "name": "platzivideo",
  "alias": ["platzivideo-api"],
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
  "env": {
    "DB_USER": "@DB_USER",
    "DB_PASSWORD": "@DB_PASSWORD",
    "DB_HOST": "@DB_HOST",
    "DB_NAME": "@DB_NAME"
  }
}
